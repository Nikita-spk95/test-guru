<tbody>
  <tr>
    <td><%= test.title %></td>
    <td><%= test.questions.count %></td>
    <td><%= test.level %>
    <td><%= test.category.title %></td>
    <td><%= test.author.first_name %> <%= test.author.last_name %></td>

    <td><%= link_to t(".show"), admin_test_path(test) %></td>
    <td><%= link_to t(".edit"), edit_admin_test_path(test) %></td>
    <td><%= link_to t(".delete"), admin_test_path(test), data: {
                      turbo_method: :delete,
                      turbo_confirm: t(".confirm")
                    } %></td>             
  </tr>
</tbody>
